<! DOCTYPE html>
    <head>
        <meta charset="utf-8">
        <title>Coding House</title>
        <link rel="stylesheet" href="problem.css">
    </head>
    <body>
        <div class="container">
            <div class="header">
                <h1><a href="index.html">CODING HOUSE</a></h1>
                <div class="nav">
                    <ul>
                        <li><a href="list">PROBLEM/CODE</a></li>
                        <li><a href="login.html">LOGIN</a></li>
                    </ul>
                </div>
            </div>
        </div>
        <div class ="board_list_wrap">
            <h2>PROBLEM / CODE</h2>
            <table class="board_list" border='1'>
                <caption>Board</caption>
                <thead>
                    <tr>
                        <th>Number</th>
                        <th>Title</th>
                        <th>Writer</th>
                    </tr>
                </thead>
                <tbody>
                    <% for(i = 0; i < list.length; i++) { %>
                    <tr>
                        <td><%=i+1 %></td>
                        <td><%=list[i].TITLE %></td>
                        <td><%=list[i].ID %></td>
                    </tr>
                    <% } %>
                </tbody>
            </table>
            <div class="paging">
                <a href="#" class="on">1</a>
            </div>
            <div class="write">
            <a href="write">Writing</a>
            </div>
        </div>
    </body>
</html>